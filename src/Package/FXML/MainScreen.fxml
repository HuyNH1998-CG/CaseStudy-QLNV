<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="541.0" prefWidth="778.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Package.FXML.MainScreenController">
   <center>
      <ListView fx:id="listView" prefHeight="427.0" prefWidth="371.0" BorderPane.alignment="CENTER" />
   </center>
   <right>
      <VBox prefHeight="444.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#switchToAdd" prefHeight="25.0" prefWidth="100.0" text="Add" />
