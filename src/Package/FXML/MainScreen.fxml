            <Button mnemonicParsing="false" onAction="#switchToSearch" prefHeight="25.0" prefWidth="100.0" text="Search" />
            <Button mnemonicParsing="false" onAction="#sort" prefHeight="25.0" prefWidth="100.0" text="Sort" />
            <Button layoutX="10.0" layoutY="135.0" mnemonicParsing="false" onAction="#getCurrentUser" prefHeight="25.0" prefWidth="100.0" text="Show User Info" />
            <Button layoutX="10.0" layoutY="160.0" mnemonicParsing="false" onAction="#switchToStatus" prefHeight="25.0" prefWidth="100.0" text="Set Status" />
